<?xml version="1.0"?>

<launch>
    <node pkg="hector_object_tracker" type="object_tracker" name="object_tracker" output="screen">
      <param name="project_objects" type="bool" value="false" />
      <param name="angle_variance" type="double" value="0.121846968"/>
      <param name="publish_interval" type="double" value="1.0" />

      <rosparam>
        number_banner_0:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.8, 0.4, 0.0]
        number_banner_1:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.8, 0.8, 0.0]
        number_banner_2:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.4, 0.8, 0.0]
        number_banner_3:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.0, 0.8, 0.0]
        number_banner_4:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.0, 0.8, 0.4]
        number_banner_5:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.0, 0.8, 0.8]
        number_banner_6:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.0, 0.4, 0.8]
        number_banner_7:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.0, 0.0, 0.8]
        number_banner_8:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.4, 0.0, 0.8]
        number_banner_9:
          active_support: 1.0
          min_height: 0.2
          max_height: 0.7
          marker_color: [0.8, 0.0, 0.8]
        target_marker:
          active_support: 1.0
          min_height: -0.2
          max_height: -0.1
          marker_color: [0.8, 0.8, 0.8]
        negative_update:
          - topic: axis215/camera_info
            min_distance: 2.0
            max_distance: 5.0
            negative_support: 0.5
      </rosparam>

      <remap from="visualization_marker" to="worldmodel/visualization_marker" />
      <remap from="visualization_marker_array" to="worldmodel/visualization_marker_array" />
    </node>

    <include file="$(find line_feature_detection)/launch/line_feature_detection.launch" />
</launch>

